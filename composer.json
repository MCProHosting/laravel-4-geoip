{
    "name": "torann/geoip",
    "description": "Supports the two main GeoIP services (infoDB and Maxmind).",
    "keywords": ["laravel", "laravel4", "geoip", "location", "geolocation"],
	"license": "BSD 2-Clause",
    "authors": [
        {
            "name": "Daniel Stainback",
            "email": "daniel@lyften.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.x",
		"geoip2/geoip2": "0.6.*",
        "illuminate/config": "4.1.*",
        "illuminate/session": "4.1.*"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "Torann\\GeoIP": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "minimum-stability": "dev"
}
